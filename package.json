{
  "name": "imdone-cli",
  "version": "1.0.0",
  "description": "A command line tool that syncs your project's TODO comments with imdone.io, github, waffle, trello, jira, twitter or anywhere!",
  "main": "index.js",
  "scripts": {
    "lint": "standard",
    "test": "npm run lint && nyc --reporter=lcov --reporter=text --reporter=html mocha"
  },
  "bin": {
    "imdone": "./index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imdone/imdone-cli.git"
  },
  "keywords": [
    "TODO",
    "issues",
    "scrum",
    "Technical-Debt",
    "FIXME"
  ],
  "author": "Jesse Piascik",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/imdone/imdone-cli/issues"
  },
  "homepage": "https://github.com/imdone/imdone-cli#readme",
  "standard": {
    "globals": [
      "before",
      "describe",
      "it",
      "should"
    ]
  },
  "devDependencies": {
    "fs.extra": "^1.3.2",
    "mocha": "^3.3.0",
    "nyc": "^10.2.0",
    "should": "^11.2.1",
    "standard": "^10.0.2"
  },
  "dependencies": {
    "async": "^2.4.0",
    "commander": "^2.9.0",
    "debug": "^2.6.8",
    "engine.io": "^3.1.0",
    "engine.io-client": "^3.1.0",
    "git-repo-info": "^1.4.1",
    "imdone-core": "^1.3.27",
    "inquirer": "^3.0.6",
    "lodash": "^4.17.4",
    "ora": "^1.2.0",
    "pm2": "^2.4.6",
    "preferences": "^0.2.1",
    "superagent": "^3.5.2",
    "superagent-no-cache": "imdone/superagent-no-cache"
  }
}
